%% Test 1: DC brush motorA 
% Test 1 tests if the time.constant x is between the specified minimum and maximum 
% value. If it isn't, assert throws an error and fails the test.

MinMotorA.timeConstant = 1/60; % [min]
MaxMotorA.timeConstant = 6/60; % [min]
x = 5/60;
assert((MinMotorA.timeConstant < x) && (x < MaxMotorA.timeConstant))
%% Test 2: DC brush motorB
% Test 2 tests if the time.constant x is between the specified minimum and maximum 
% value. If it isn't, assert throws an error and fails the test.
MinMotorA.timeConstant = 7/60; % [min]
MaxMotorA.timeConstant = 13/60; % [min]
x = 10/60;
assert((MinMotorA.timeConstant < x) && (x < MaxMotorA.timeConstant))
%% Test 3: DC brush motorC
% Test 3 tests if the time.constant x is between the specified minimum and maximum 
% value. If it isn't, assert throws an error and fails the test.
MinMotorA.timeConstant = 14/60; % [min]
MaxMotorA.timeConstant = 19/60; % [min]
x = 10/60;
assert((MinMotorA.timeConstant < x) && (x < MaxMotorA.timeConstant)) 
